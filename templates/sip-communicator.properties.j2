org.jitsi.jicofo.ALWAYS_TRUST_MODE_ENABLED=true

org.ice4j.ice.harvest.NAT_HARVESTER_LOCAL_ADDRESS={{internal_ip}}
org.ice4j.ice.harvest.NAT_HARVESTER_PUBLIC_ADDRESS={{external_ip}}
org.jitsi.videobridge.SINGLE_PORT_HARVESTER_PORT=10000

org.jitsi.videobridge.ENABLE_REST_SHUTDOWN=true

org.jitsi.videobridge.rest.private.jetty.port=80

org.jitsi.videobridge.xmpp.user.shard.HOSTNAME={{prosody_ip}}
org.jitsi.videobridge.xmpp.user.shard.DOMAIN=auth.{{jitsi_domain}}
org.jitsi.videobridge.xmpp.user.shard.USERNAME=jvb
org.jitsi.videobridge.xmpp.user.shard.PASSWORD={{jvb_auth_password}}
org.jitsi.videobridge.xmpp.user.shard.MUC_JIDS=JvbBrewery@internal.auth.{{jitsi_domain}}
org.jitsi.videobridge.xmpp.user.shard.MUC_NICKNAME={{inventory_hostname}}
org.jitsi.videobridge.xmpp.user.shard.DISABLE_CERTIFICATE_VERIFICATION=true

# Enable broadcasting stats/presence in a MUC
org.jitsi.videobridge.ENABLE_STATISTICS=true
org.jitsi.videobridge.STATISTICS_TRANSPORT=muc,colibri,rest
org.jitsi.videobridge.STATISTICS_INTERVAL=5000
